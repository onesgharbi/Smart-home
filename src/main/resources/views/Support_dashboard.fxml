    </AnchorPane>

    <!-- Sidebar Section -->
    <VBox alignment="TOP_CENTER" layoutY="55.0" prefHeight="613.0" prefWidth="227.0" style="-fx-background-color: #f0e5e0; -fx-padding: 20px; -fx-spacing: 15px;">
        <Button prefWidth="200" style="-fx-background-color: #fff; -fx-border-color: #d1b1a1; -fx-font-size: 14px;" text="Dashboard" />
        <Button prefWidth="200" style="-fx-background-color: #fff; -fx-border-color: #d1b1a1; -fx-font-size: 14px;" text="Devices" />
