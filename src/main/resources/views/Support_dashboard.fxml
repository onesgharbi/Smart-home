    </VBox>

    <!-- Main Content Section -->
    <VBox layoutX="250.0" layoutY="70.0" spacing="20.0" alignment="TOP_LEFT">
        <!-- Contact Support Information -->
        <Label style="-fx-font-size: 20px; -fx-font-weight: bold; -fx-text-fill: #333;" text="Contact Support" />
        <Label style="-fx-font-size: 14px; -fx-text-fill: #666;" text="For assistance, please contact us at:" />

        <HBox spacing="10" alignment="TOP_LEFT">
            <Label style="-fx-font-size: 14px; -fx-text-fill: #333;" text="Email: " />
            <Label style="-fx-font-size: 14px; -fx-text-fill: #1a73e8;" text="support@example.com" />
        </HBox>

        <HBox spacing="10" alignment="TOP_LEFT">
            <Label style="-fx-font-size: 14px; -fx-text-fill: #333;" text="Phone: " />
            <Label style="-fx-font-size: 14px; -fx-text-fill: #1a73e8;" text="+1-800-123-4567" />
        </HBox>

        <!-- Additional Information Section -->
        <VBox spacing="15.0" alignment="TOP_LEFT">
            <Label style="-fx-font-size: 16px; -fx-font-weight: bold; -fx-text-fill: #333;" text="Quick Links" />
            <Button style="-fx-background-color: #fff; -fx-border-color: #d1b1a1; -fx-font-size: 14px;" text="FAQ" />
            <Button style="-fx-background-color: #fff; -fx-border-color: #d1b1a1; -fx-font-size: 14px;" text="Support Articles" />
            <Button style="-fx-background-color: #fff; -fx-border-color: #d1b1a1; -fx-font-size: 14px;" text="Live Chat" />
